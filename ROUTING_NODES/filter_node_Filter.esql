

CREATE FILTER MODULE filter_node_Filter
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
DECLARE employe REFERENCE TO Root.JSON.Data.employe;
		
		IF employe.id = 1266 THEN
			
			RETURN True;
		
		ELSEIF employe.id = 1254 THEN
			RETURN Unknown;
		
		ELSEIF employe.id = 1284 THEN
			RETURN False;
			
			
		END IF;
		
		
RETURN TRUE;

	END;

END MODULE;
